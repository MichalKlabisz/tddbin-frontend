{
  "name": "tddbin-frontend",
  "version": "0.0.1",
  "description": "TDDbin.com UI components (and examples)",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && jasmine-node src",
    "lint": "jsxcs --preset google src",
    "build": "browserify examples/tddbin-standalone.js --outfile=dist/tddbin-standalone.js --transform=reactify",
    "watch": "watchify examples/tddbin-standalone.js --outfile=dist/tddbin-standalone.js --transform=reactify -v",
    "start": "webpack-dev-server",
    "deploy": "npm run build && ./deploy-ghpages.sh"
  },
  "browserify": {
    "extension": [
      "jsx"
    ],
    "transform": [
      [
        "reactify",
        {
          "es6": true
        }
      ]
    ]
  },
  "keywords": [
    "tddbin",
    "tdd",
    "javascript"
  ],
  "bugs": {
    "url": "https://github.com/uxebu/tddbin-frontend/issues"
  },
  "author": "uxebu",
  "license": "MIT",
  "dependencies": {
    "mocha": "^1.21.3",
    "referee": "^1.0.3",
    "should": "^4.0.4"
  },
  "devDependencies": {
    "ace-with-plugins": "git://github.com/uxebu/ace-with-plugins.git",
    "browserify": "^8.1.3",
    "jasmine-matchers": "^0.2.3",
    "jasmine-node": "^1.14.5",
    "jsxcs": "0.0.3",
    "reactify": "^1.0.0",
    "watchify": "^2.3.0",
    "react": "^0.12.2"
  }
}
